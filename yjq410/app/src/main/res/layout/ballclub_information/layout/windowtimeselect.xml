<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#66000000"
    android:gravity="center">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="320dp"
        android:orientation="vertical"
        android:background="#ffffff">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <RelativeLayout
                android:id="@+id/select_time"
                android:layout_width="match_parent"
                android:layout_height="40dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:src="@drawable/showselectday"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textSize="15sp"
                        android:layout_marginLeft="20dp"
                        android:text="选时日期:"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true">
                    <TextView
                        android:id="@+id/windowtimeselect_text_day"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="10sp"
                        android:layout_marginRight="25dp"
                        android:text="未选择"/>
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <CheckBox
                android:id="@+id/radioOne"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="30dp"
                style="@style/select_style"
                android:text="2019年4月19日"
                android:tag="2019年4月19日"/>
            <CheckBox
                android:id="@+id/radioTwo"
                android:layout_width="0dp"
                android:layout_weight="1.09"
                android:layout_height="30dp"
                style="@style/select_style"
                android:text="2019年4月20日"
                android:tag="2019年4月20日"/>
            <CheckBox
                android:id="@+id/radioThree"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="30dp"
                style="@style/select_style"
                android:text="2019年4月20日"
                android:tag="2019年4月20日"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:src="@drawable/showselecttime"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textSize="15sp"
                        android:layout_marginLeft="20dp"
                        android:text="选时时间:"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true">
                    <TextView
                        android:id="@+id/windowtimeselect_text_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="10sp"
                        android:layout_marginRight="25dp"
                        android:text="未选择"/>
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:gravity="center">
                <CheckBox
                    android:id="@+id/checkboxOne"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="07:00~08:00"
                    android:textSize="10sp"
                    android:tag="07:00~08:00"/>
                <CheckBox
                    android:id="@+id/checkboxTwo"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="08:00~09:00"
                    android:textSize="10sp"
                    android:tag="08:00~09:00"/>
                <CheckBox
                    android:id="@+id/checkboxThree"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="09:00~10:00"
                    android:textSize="10sp"
                    android:tag="09:00~10:00"/>
                <CheckBox
                    android:id="@+id/checkboxFour"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="10:00~11:00"
                    android:textSize="10sp"
                    android:tag="10:00~11:00"/>
                <CheckBox
                    android:id="@+id/checkboxFive"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    style="@style/select_style"
                    android:gravity="center"
                    android:text="11:00~12:00"
                    android:textSize="10sp"
                    android:tag="11:00~12:00"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:gravity="center">
                <CheckBox
                    android:id="@+id/checkboxSex"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="12:00~13:00"
                    android:textSize="10sp"
                    android:tag="12:00~13:00"/>
                <CheckBox
                    android:id="@+id/checkboxServen"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="13:00~14:00"
                    android:textSize="10sp"
                    android:tag="13:00~14:00"/>
                <CheckBox
                    android:id="@+id/checkboxEight"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="14:00~15:00"
                    android:textSize="10sp"
                    android:tag="14:00~15:00"/>
                <CheckBox
                    android:id="@+id/checkboxNight"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="15:00~16:00"
                    android:textSize="10sp"
                    android:tag="15:00~16:00"/>
                <CheckBox
                    android:id="@+id/checkboxTen"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="16:00~17:00"
                    android:textSize="10sp"
                    android:tag="16:00~17:00"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:gravity="center">
                <CheckBox
                    android:id="@+id/checkboxTenOne"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:textSize="10sp"
                    android:text="17:00~18:00"
                    android:tag="17:00~18:00"/>
                <CheckBox
                    android:id="@+id/checkboxTenTwo"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="18:00~19:00"
                    android:textSize="10sp"
                    android:tag="18:00~19:00"/>
                <CheckBox
                    android:id="@+id/checkboxTenThree"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="19:00~20:00"
                    android:textSize="10sp"
                    android:tag="19:00~20:00"/>
                <CheckBox
                    android:id="@+id/checkboxTenFour"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="20:00~21:00"
                    android:textSize="10sp"
                    android:tag="20:00~21:00"/>
                <CheckBox
                    android:id="@+id/checkboxTenFive"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    style="@style/select_style"
                    android:text="21:00~22:00"
                    android:textSize="10sp"
                    android:tag="21:00~22:00"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <Button
                    android:id="@+id/windowtimeselect_cancel"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#ffffff"
                    android:text="取消"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <Button
                    android:id="@+id/windowtimeselect_submit"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFF66"
                    android:text="确定"
                    android:textColor="#ffffff"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>